function n=n_elements(var)
%function n_elements(var)
%******** **********
%**

if isa(var,'i2mstr'); var=var(:); end;
res=size(var); c=ischar(var); s=length(res);
n  =1;
if  c; res(2)=1; if isempty(var) & ~isa(var,'i2mstr'); n=0; end; return; end;

for i=1:s; n=n*res(i); end;

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

